{
  "name": "@trio/dynamic-module-federation",
  "version": "0.0.1",
  "description": "React hook for webpack module federation",
  "main": "dist/umd/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint --fix --ignore-path .gitignore src",
    "lint:prettier": "pretty-quick",
    "commit": "git cz",
    "test": "echo \"no tests found\" && exit 0",
    "semantic-release": "npx --package semantic-release@^17.0.4 semantic-release",
    "build": "rollup -c --environment NODE_ENV:production"
  },
  "keywords": [
    "module federation",
    "hooks",
    "webpack"
  ],
  "author": "Trio team",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-react": "^7.23.3",
    "@commitlint/config-conventional": "^18.6.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "babel-eslint": "^10.1.0",
    "commitlint": "^18.6.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.7",
    "lint-staged": "^15.2.1",
    "prettier": "^3.2.4",
    "rollup": "^4.9.6",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2"
  },
  "peerDependencies": {
    "prop-types": ">=15.1.0",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  }
}
